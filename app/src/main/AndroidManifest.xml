<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.demo.landbankregistration">

    <application
        android:name=".LandbankRegistrationApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FaceRecognition"
            android:label="Face Recognition"></activity>
        <activity android:name=".ScanResult" />
        <activity
            android:name=".ScanDocumentsActivity"
            android:label="Scan Documents" />
        <activity
            android:name=".SelectIDActivity"
            android:label="Select Documents" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>